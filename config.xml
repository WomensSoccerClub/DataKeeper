            <database>"womensso_wsc"</database>
            <!-- <column name="Member ID" datatype="int">MEMBER_IDENTIFIER</column> -->
            <column name="Primary Name" datatype="varchar">PRIMARY_NAME</column>
            <column name="Primary Phone" datatype="varchar">PRIMARY_PHONE</column>
            <column name="Secondary Name" datatype="varchar">SECONDARY_NAME</column>
            <column name="Secondary Phone" datatype="varchar">SECONDARY_PHONE</column>
        </table>
