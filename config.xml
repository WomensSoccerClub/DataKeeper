<?xml version="1.0" encoding="UTF-8"?>
<!--
The following is the configuration for how the DataKeeper tool will associate the data within the databases
-->
<KeyObjects>
    <KeyObject id="0" name="Member">
        
        <table name="Member" value="T_MEMBER">
            <database>"womensso_wsc"</database>
            <column name="Member ID" datatype="int">MEMBER_IDENTIFIER</column>
            <column name="Username" datatype="varchar">USERNAME</column>
            <column name="Last Name" datatype="varchar">LAST_NAME</column>
            <column name="Middle Initial" datatype="char">MIDDLE_INITIAL_TEXT</column>
            <column name="First Name" datatype="varchar">FIRST_NAME</column>
            <column name="Birthdate" datatype="date">BIRTH_DATE</column>
            <column name="Most Recent Level" datatype="char" class="lookup" lookupTable="T_LEVEL" >MOST_RECENT_LEVEL_CODE_CHAR</column>
            <column name="Total Years Experience" datatype="char">TOTAL_EXPERIENCE_YEARS_CATEGORY_CODE</column>      
            <column name="Last Years Played" datatype="char">LAST_PLAYED_YEARS_CATEGORY_CODE</column>
            <column name="Number of Children" datatype="tinyint">NUMBER_OF_CHILDREN_QUANTITY</column>
            <column name="Photograph" datatype="varchar">PHOTOGRAPH_DIRECTORY_TEXT</column>
            <column name="Date Enrolled" datatype="datetime">MEMBER_ENROLLMENT_DATETIME</column>
            <column name="Date Last Updated" datatype="datetime">MEMBER_LAST_UPDATE_DATETIME</column>
            <column name="Health Insurance Indicator" datatype="char">HEALTH_INSURANCE_INDICATOR</column>
        </table>   
        <table name="Health Insurance" value="T_HEALTH_INSURANCE">
            <database>"womensso_wsc"</database>
           <!-- <column name="Member ID" datatype="int">MEMBER_IDENTIFIER</column> -->
           <!-- <column name="Order Level Code" datatype="char">ORDER_LEVEL_CODE</column> -->
            <column name="Company Name" datatype="varchar">COMPANY_NAME</column>
            <column name="Holder Name" datatype="varchar">HOLDER_NAME</column>
            <column name="Policy ID" datatype="varchar">POLICY_IDENTIFIER</column>
            <column name="Group ID" datatype="varchar">HOLDER_NAME</column>
        </table>        
        <table name="Volunteer Opportunity" value="T_VOLUNTEER_OPPORTUNITY">
            <database>"womensso_wsc"</database>
            <!-- <column name="Member ID" datatype="int">MEMBER_IDENTIFIER</column> -->
            <column name="Opportunities" datatype="varchar" class="lookup" lookupTable="T_VOLUNTEER_OPPORTUNITY_CODE">VOLUNTEER_OPPORTUNITY_CODES</column>
        </table>
        <table name="Address" value="T_ADDRESS">
            <database>"womensso_wsc"</database>
            <!-- <column name="Member ID" datatype="int">MEMBER_IDENTIFIER</column> -->
            <!-- <column name="Order Level Code" datatype="char">ORDER_LEVEL_CODE</column> -->
            <column name="Street Address Line 1" datatype="varchar">STREET_ADDRESS_LINE1_TEXT</column>
            <column name="Street Address Line 2" datatype="varchar">STREET_ADDRESS_LINE2_TEXT</column>
            <column name="City" datatype="varchar">CITY_TEXT</column>
            <column name="State" datatype="varchar" class="lookup" lookupTable="T_STATE">T_STATE_ABBREVIATION_CODE</column>
            <column name="Zip Code" datatype="char">FIVE_DIGIT_POSTAL_CODE</column>
        </table>
        <table name="Occupation" value="T_OCCUPATION">
            <database>"womensso_wsc"</database>
